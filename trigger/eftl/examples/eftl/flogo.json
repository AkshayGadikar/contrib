{
  "name": "EFTL_Trigger",
  "type": "flogo:app",
  "version": "1.0.0",
  "description": "This is a EFTL Trigger",
  "properties": null,
  "channels": null,
  "triggers": [
    {
      "name": "tibco-eftl",
      "id": "flogo-eftl",
      "ref": "github.com/project-flogo/contrib/trigger/eftl",
      "settings": {
        "tracer": "zipkin",
        "tracerEndpoint": "http://localhost:9411/api/v1/spans"
        "url": "ws://localhost:9191/channel",
        "id": "eftl",
        "user": "",
        "password": ""
      },
      "handlers": [
        {
          "settings": {
            "dest": "GET",
            "Condition": "/{triggerName}/swagger"
          },
          "actions": [
            {
              "id": "swaggertrigger"
            }
          ]
        }
      ]
    }
  ],
  "resources": [
    {
      "id": "microgateway:jwt",
      "compressed": false,
      "data": {
        "name": "Pets",
        "steps": [
          {
            "service": "PetStorePets",
            "input": {
              "pathParams.petId": "1"
            }
          }
        ],
        "responses": [],
        "services": [
          {
            "name": "PetStorePets",
            "description": "Get pets by ID from the petstore",
            "ref": "github.com/project-flogo/contrib/activity/rest",
            "settings": {
              "uri": "https://petstore.swagger.io/v2/pet/:petId",
              "method": "GET"
            }
          }
        ]
      }
    }
  ],
  "actions": [
    {
      "ref": "github.com/project-flogo/microgateway",
      "settings": {
        "uri": "microgateway:jwt"
      },
      "id": "swaggertrigger",
      "metadata": null
    },
    {
      "ref": "github.com/project-flogo/microgateway",
      "settings": {
        "uri": "microgateway:jwt"
      },
      "id": "microgateway:jwt",
      "metadata": null
    }
  ]
}